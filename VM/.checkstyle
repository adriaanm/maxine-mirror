<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="true">
    <local-check-config name="Maxine Checks" location="/Base/checkstyle/maxine_checks.xml" type="project" description="">
        <additional-data name="protect-config-file" value="false"/>
    </local-check-config>
    <fileset name="all" enabled="true" check-config-name="Maxine Checks" local="true">
        <file-match-pattern match-pattern="." include-pattern="true"/>
    </fileset>
    <filter name="FilesFromPackage" enabled="true">
        <filter-data value="test/test/bytecode"/>
        <filter-data value="test/test/except"/>
        <filter-data value="test/test/interactive"/>
        <filter-data value="test/test/jdk"/>
        <filter-data value="test/test/lang"/>
        <filter-data value="test/test/micro"/>
        <filter-data value="test/test/optimize"/>
        <filter-data value="test/test/reflect"/>
        <filter-data value="test/test/threads"/>
    </filter>
    <filter name="NonSrcDirs" enabled="true"/>
</fileset-config>
