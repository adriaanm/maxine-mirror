<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="true">
    <local-check-config name="Maxine Checks" location="/Base/.checkstyle_checks.xml" type="project" description="">
        <additional-data name="protect-config-file" value="false"/>
    </local-check-config>
    <fileset name="all" enabled="true" check-config-name="Maxine Checks" local="true">
        <file-match-pattern match-pattern="." include-pattern="true"/>
    </fileset>
    <filter name="FilesFromPackage" enabled="true">
        <filter-data value="src/com/sun/max/asm/amd64/complete"/>
        <filter-data value="src/com/sun/max/asm/ia32/complete"/>
    </filter>
    <filter name="NonSrcDirs" enabled="true"/>
</fileset-config>
